generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        Int      @id @default(autoincrement())
  outcome   String
  type      String 
  amount    Int
  date      DateTime? @db.Date 
  note      String?
  timestamp BigInt
  createdAt DateTime @default(now())
}
